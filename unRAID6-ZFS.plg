<!-- Install package based on kernel  -->
<FILE Run="/bin/bash">
<INLINE>

# source unRAID version
. /etc/unraid-version

# kernel version
kernel_version=$(uname -a)

download_install() {
  local dest="&plugin;/packages/${1}"
  local src="&repo;/packages/${1}"
  local md5=$2
  mkdir -p "&plugin;/packages"
  if [ ! -f "${dest}" ]; then
    curl --location --silent --fail "${src}" --output "${dest}"
  fi
  file_md5=$(/usr/bin/md5sum ${dest})
  if [ "${file_md5:0:32}" != "${md5:0:32}" ]; then
    echo "Wrong '${1}' package md5 hash."
    rm "${dest}"
    exit 1
  else
    /sbin/installpkg "$dest"

    #delte old tgz
    if [[ $kernel_version == *"4.4.30-unRAID"* ]]; then
      find "&plugin;/packages/"* ! -name "*6.2.4*" -exec rm {} \;
    else
      find "&plugin;/packages/"* ! -name *"$version"* -exec rm {} \;
    fi
  fi
}

# Version 6.3.rc9
if [[ $kernel_version == *"4.9.6-unRAID"* ]]; then
  spl_pkg="spl-0.7.0-rc3-unRAID6.3.rc9.x86_64.tgz"
  spl_md5="36ea2c4cb9f6c6652184f35870c249dc"
  zfs_pkg="zfs-0.7.0-rc3-unRAID6.3.rc9.x86_64.tgz"
  zfs_md5="e78283fed9d50cecc9956226a46f15cb"
  supported_kernel=true

# Version 6.3.rc6
elif [[ $kernel_version == *"4.8.12-unRAID"* ]]; then
  spl_pkg="spl-0.6.5.8-unRAID6.3.rc6.x86_64.tgz"
